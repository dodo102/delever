<AnchorPane prefHeight="600.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.deliver.Chat">
   <children>
      <Pane layoutX="50.0" layoutY="146.0" prefHeight="229.0" prefWidth="300.0">
         <children>
            <Label layoutX="10.0" layoutY="40.0" text="방 제목">
               <font>
                  <Font name="System Italic" size="20.0" />
               </font>
            </Label>
            <PasswordField fx:id="roomPW" layoutX="103.0" layoutY="110.0" prefHeight="30.0" prefWidth="175.0" />
            <TextField fx:id="roomName" layoutX="103.0" layoutY="40.0" prefHeight="30.0" prefWidth="175.0" />
            <Label fx:id="pwLa" layoutX="10.0" layoutY="110.0" text="비밀번호">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <CheckBox fx:id="hide" layoutX="76.0" layoutY="173.0" mnemonicParsing="false" onAction="#hideRoom" prefHeight="42.0" prefWidth="158.0" text="비밀방" />
         </children>
      </Pane>
      <Button fx:id="make" layoutX="169.0" layoutY="453.0" mnemonicParsing="false" onAction="#insert" text="만들기">
